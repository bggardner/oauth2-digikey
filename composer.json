{
    "name": "bggardner/oauth2-digikey",
    "description": "Digi-Key OAuth 2.0 Client Provider for the PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Gardner",
            "email": "brent@ebrent.net",
            "homepage": "https://github.com/bggardner"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "digikey",
        "digi-key"
    ],
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.5",
        "php-parallel-lint/php-parallel-lint": "^1.3.1",
        "phpunit/phpunit": "^5.7 || ^6.0 || ^9.5",
        "squizlabs/php_codesniffer": "^2.3 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bggardner\\OAuth2\\Client\\": "src/"
        }
    },
    "audoload-dev": {
        "psr-4": {
            "Bggardner\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
